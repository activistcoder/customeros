type EmailInfo {
    id: ID!
    email: String!
    label: EmailLabel!
    primary: Boolean!
}

input EmailInput {
    email: String!
    label: EmailLabel!
    primary: Boolean
}

input EmailUpdateInput {
    id: ID!
    emailDetails: EmailInput!
}

enum EmailLabel {
    MAIN
    WORK
    HOME
    OTHER
}
