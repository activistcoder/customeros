union Action = PageViewAction | MessageAction

type PageViewAction implements Node {
    id: ID!
    startedAt: Time!
    endedAt: Time!
    pageTitle: String!
    pageUrl: String!
    application: String!
    sessionId: ID!
    orderInSession: Int64!
    engagedTime: Int64!
}

type MessageAction implements Node {
    id: ID!
    startedAt: Time!
    channel: MessageChannel!
    conversationId: ID!
}

enum ActionType {
    PAGE_VIEW
    MESSAGE
}